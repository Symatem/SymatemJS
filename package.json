{
  "name": "SymatemWasm",
  "version": "0.0.0-semantic-release",
  "authors": [
    "Alexander MeiÃŸner",
    "Markus Felten"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/Symatem/SymatemWasm.git"
  },
  "engines": {
    "node": ">=8.1"
  },
  "description": "Symatem WebAssembly API",
  "main": "dist/node-interface.js",
  "module": "src/node-interface.js",
  "dependencies": {},
  "devDependencies": {
    "ava": "^0.19.1",
    "babel-preset-es2015-rollup": "^3.0.0",
    "nyc": "^11.0.2",
    "rollup": "^0.42.0",
    "rollup-plugin-babel": "^2.7.1",
    "rollup-plugin-multi-entry": "^2.0.1",
    "semantic-release": "^6.3.6"
  },
  "scripts": {
    "cover": "nyc ava",
    "test": "ava",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post",
    "prepublish": "rollup --output=dist/node-interface.js -c -- src/node-interface.js",
    "pretest": "rollup -c tests/rollup.config.js",
    "precover": "rollup -c tests/rollup.config.js"
  },
  "ava": {
    "files": [
      "build/test-bundle.js"
    ],
    "require": [
      "babel-register"
    ]
  },
  "nyc": {
    "include": [
      "build/test-bundle.js"
    ],
    "reporter": [
      "lcov"
    ],
    "report-dir": "./build/coverage"
  },
  "xo": {
    "space": true
  }
}
